@article{Abdallah2015a,
abstract = {Wind tunnel test measurements to characterize the static lift and drag coefficients of airfoils used in wind turbine blades are shown to possess large uncertainties, which leads to uncertainties in the aerodynamic loads on the rotor. In this paper a rational stochastic model is proposed to quantify the uncertainty in airfoil static lift and drag coefficients based on field and wind tunnel data, aero-servo-elastic calculations and engineering judgment. The stochastic model is subsequently used to assess the effect of the uncertainty in airfoil static lift and drag coefficients on the prediction of extreme loads and structural reliability of large wind turbines. It is shown that the uncertainty in the static airfoil data has a significant impact on the prediction of extreme loads effects and structural reliability depending on the component, operating conditions (stand-still versus power production) and the correlations of aerodynamic variables along the span of the blades.},
author = {Abdallah, I. and Natarajan, A. and S{\o}rensen, J. D.},
doi = {10.1016/j.renene.2014.10.009},
file = {:Users/sanderse/Dropbox/work/Mendeley/Abdallah, Natarajan, S{\o}rensen/Renewable Energy/Abdallah, Natarajan, S{\o}rensen - 2015 - Impact of uncertainty in airfoil characteristics on wind turbine extreme loads(2).pdf:pdf},
issn = {18790682},
journal = {Renewable Energy},
keywords = {Aerodynamic stochastic model,Airfoil aerodynamics uncertainty,Extreme wind loads,Probabilistic modeling,Structural reliability,Wind turbines},
pages = {283--300},
publisher = {Elsevier Ltd},
title = {{Impact of uncertainty in airfoil characteristics on wind turbine extreme loads}},
url = {http://dx.doi.org/10.1016/j.renene.2014.10.009},
volume = {75},
year = {2015}
}
@article{Sayed2019,
abstract = {Currently, the wind turbine size is increasing dramatically, and the blades are experiencing large deformations. Accordingly, the aerodynamic force distributions over the blades are changed, and hence aeroelastic analysis has become of great importance. The state-of-the-art simulation tools for wind turbines aeroelasticity utilize engineering models to find the aeroelastic response. These tools use simplified methods such as BEM to find the unsteady aerodynamic loads and 1D structural models to determine the deformations. They are computationally cheap, but they are based on different corrections to account for the unsteadiness and the 3D effects. These corrections might lead to a decrease in model accuracy. Therefore, the objective of the present studies is to compare the results of engineering models to CFD-based aeroelastic simulations that do require less empirical modeling. The Multi-Body Simulation (MBS) solver SIMPACK is used to determine the dynamic response of the rotor and the blade aerodynamic loads were calculated by an integrated third-party module (AeroDyn) based on BEM. The block-structured CFD solver FLOWer is utilized to obtain the aerodynamic blade loads based on the time-accurate solution of the unsteady Reynolds-averaged Navier-Stokes equations. The engineering model predicted smaller power and thrust compared to the values obtained using the high-fidelity CFD-based aeroelastic model. Moreover, 1{\%}–1.5{\%} increase in the power and thrust was predicted from the engineering model by increasing the number of polars used along the blade.},
author = {Sayed, M. and Klein, L. and Lutz, Th and Kr{\"{a}}mer, E.},
doi = {10.1016/j.renene.2019.03.046},
file = {:Users/sanderse/Dropbox/work/Mendeley/Sayed et al/Renewable Energy/Sayed et al. - 2019 - The impact of the aerodynamic model fidelity on the aeroelastic response of a multi-megawatt wind turbine.pdf:pdf},
issn = {18790682},
journal = {Renewable Energy},
keywords = {Aeroelasticity,Engineering models,High-fidelity,Wind turbine},
pages = {304--318},
title = {{The impact of the aerodynamic model fidelity on the aeroelastic response of a multi-megawatt wind turbine}},
volume = {140},
year = {2019}
