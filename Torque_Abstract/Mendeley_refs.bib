@article{Meyer2010,
abstract = {We propose a conservative, second-order accurate immersed interface method for representing incompressible fluid flows over complex three dimensional solid obstacles on a staggered Cartesian grid. The method is based on a finite-volume discretization of the incompressible Navier-Stokes equations which is modified locally in cells that are cut by the interface in such a way that accuracy and conservativity are maintained. A level-set technique is used for description and tracking of the interface geometry, so that an extension of the method to moving boundaries and flexible walls is straightforward. Numerical stability is ensured for small cells by a conservative mixing procedure. Discrete conservation and sharp representation of the fluid-solid interface render the method particularly suitable for Large-Eddy Simulations of high-Reynolds number flows. Accuracy, second-order grid convergence and robustness of the method is demonstrated for several test cases: inclined channel flow at Re= 20, flow over a square cylinder at Re= 100, flow over a circular cylinder at Re= 40, Re= 100 and Re= 3900, as well as turbulent channel flow with periodic constrictions at Re= 10,595. {\textcopyright} 2010 Elsevier Inc.},
author = {Meyer, M. and Devesa, A. and Hickel, S. and Hu, X. Y. and Adams, N. A.},
doi = {10.1016/j.jcp.2010.04.040},
file = {:Users/sanderse/Dropbox/work/Mendeley/Meyer et al/Journal of Computational Physics/Meyer et al. - 2010 - A conservative immersed interface method for Large-Eddy Simulation of incompressible flows.pdf:pdf},
isbn = {0021-9991},
issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {Immersed boundary method,Immersed interface method,Large-Eddy Simulation},
number = {18},
pages = {6300--6317},
title = {{A conservative immersed interface method for Large-Eddy Simulation of incompressible flows}},
volume = {229},
year = {2010}
}
@article{DAlessandro2017a,
