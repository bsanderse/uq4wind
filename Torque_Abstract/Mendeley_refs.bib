@article{Danielson2011,
abstract = {Abstract This paper presents test cases, methodologies, and test results from the extensive testing and qualification program that have been conducted with LedaFlow{\textregistered}. This new transient multiphase flowline simulator has been built with focus on multiphase flow challenges in the oil and gas industry, and the resulting software includes both transient 1D and quasi 3D multiphase models. Development has been based both on existing and new large-scale experimental data from the SINTEF multiphase laboratory, taken over a nearly ten-year period. The focus of this paper is the transient 1D model. The transient three-phase 1D simulator offers new modeling capabilities as it is based on a model concept conserving mass for 9 fields (bubbles, droplets, and continuous fluids), using three momentum equations, and solving for the enthalpy and temperature of the three individual fluid zones. Improved closure relations have been built from a combination of new large-scale multiphase flow experiments and sub-models which represents the distribution of droplets over the pipe cross section. Before introduction of this new tool to the industry, it was taken through an extensive testing and qualification program. The testing included comparison with experimental data (over 10,000 experimental points) and field data (over a dozen pipelines and wells, from wet-gas to oil), as well as comparisons to other commercially available software. Another part of the testing was focused on typical flow assurance user cases; transient simulations such as turn-down, shut-down, start-up and depressurization were run for a number of realistic field geometries. The new multiphase flow simulator reproduces experimental data and field data within defined acceptance criteria. This new multiphase flow software can be used as an alternative to other commercially available software and as a valuable new tool for flow assurance work for field development, operational studies and decision support. Introduction The development of the new transient multiphase flow simulator, with both one- and multi-dimensional capabilities has been ongoing for almost a decade. The goal of the development has been to meet industry needs for improved and alternative simulation of fluids management and related flow assurance issues in oil/gas production and processing operations. LedaFlow{\textregistered} is composed of a suite of models, including a complete one-dimensional transient model with compositional tracking and network solver. Figure 1 illustrates the three main phases of the development project and focus areas in each phase. The first phase of the project was primarily focused on extensive testing at the SINTEF Multiphase Flow Laboratory. The extensive test campaign was conducted in a 12-inch loop for 2-phase flow, and an 8-inch loop for 3-phase flow (Danielson et al, 2005). New 1D data was acquired for hold-up, pressure gradient, and flow regime. These data are exclusive available for development of the new multiphase simulator and filled data gaps in the existing SINTEF Tilda database, which now in total includes more than 10,000 data points.},
author = {Danielson, Thomas John and Bansal, Krishan M. and Djoric, Biljana and Duret, Emmanuel-Denis and Johansen, Stein Tore and Hellan, Oyvind},
doi = {10.4043/21417-MS},
file = {:Users/sanderse/Dropbox/work/Mendeley/Danielson et al/Offshore Technology Conference/Danielson et al. - 2011 - Testing and Qualification of a New Multiphase Flow Simulator.pdf:pdf},
isbn = {978-1-61399-117-6},
journal = {Offshore Technology Conference},
pages = {1--16},
title = {{Testing and Qualification of a New Multiphase Flow Simulator}},
url = {http://op.spe.semcs.net/doi/10.4043/21417-MS},
year = {2011}
}
@article{Pouraria2016,
abstract = {The concurrent flow of oil and water in pipelines is a common occurrence in offshore oil production systems. The internal structure of the oil-water flow, known as the flow pattern, and the distribution of water have a great influence on the design of the pipeline. However, due to the complex nature of oil-water flows, predicting flow patterns under different operating conditions is a challenging task. The present study is an attempt toward using a computational fluid dynamics model for predicting the flow patterns under different working conditions. To this end, an Eulerian-Eulerian model along with appropriate closure laws was employed to model two-phase oil-water flows through a horizontal pipe. The standard k-epsilon model was adopted to account for the turbulence effects. Furthermore, a brief description of the main interfacial forces namely the drag, lift, and turbulent dispersion forces has been presented. The numerical model was used to predict the flow patterns under different operating conditions ranging from low to high velocities and water cuts, respectively. A comparison among the obtained numerical results and published experimental data showed reasonable agreement.},
author = {Pouraria, Hassan and Seo, Jung Kwan and Paik, Jeom Kee},
doi = {10.1016/j.oceaneng.2016.02.007},
file = {:Users/sanderse/Dropbox/work/Mendeley/Pouraria, Seo, Paik/Ocean Engineering/Pouraria, Seo, Paik - 2016 - Numerical modelling of two-phase oil-water flow patterns in a subsea pipeline.pdf:pdf},
issn = {00298018},
journal = {Ocean Engineering},
keywords = {CFD,Flow pattern,Phase distribution,Subsea pipeline,Two-phase flow,Water wetting},
pages = {135--148},
publisher = {Elsevier},
title = {{Numerical modelling of two-phase oil-water flow patterns in a subsea pipeline}},
url = {http://dx.doi.org/10.1016/j.oceaneng.2016.02.007},
volume = {115},
year = {2016}
}
