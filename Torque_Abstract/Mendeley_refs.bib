doi = {10.1006/jcph.1999.6187},
file = {:Users/sanderse/Dropbox/work/Mendeley/Saurel, Abgrall/Journal of Computational Physics/Saurel, Abgrall - 1999 - A Multiphase Godunov Method for Compressible Multifluid and Multiphase Flows.pdf:pdf},
