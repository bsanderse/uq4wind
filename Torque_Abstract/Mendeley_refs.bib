journal = {Journal of Computational and Applied Mathematics},
keywords = {Eigenvalue search method,Hyperbolic-tangent shear flow,Orr-Sommerfeld equation},
number = {11},
pages = {2795--2802},
publisher = {Elsevier B.V.},
title = {{An eigenvalue search method using the Orr-Sommerfeld equation for shear flow}},
url = {http://dx.doi.org/10.1016/j.cam.2012.01.012},
