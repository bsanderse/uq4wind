isbn = {0021-9991},
issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {Adaptive sparse grids,Generalized polynomial chaos method,High-dimensional approximation,Multivariate discontinuity detection,Stochastic partial differential equations},
number = {10},
