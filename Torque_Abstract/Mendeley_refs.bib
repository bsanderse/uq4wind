journal = {Collection of Technical Papers - 7th AIAA Aviation Technology, Integration, and Operations Conference},
number = {September},
pages = {96--127},
title = {{Recent extensions and applications of the "CST" universal parametric geometry representation method}},
volume = {1},
year = {2007}
}
@article{Muller2018,
abstract = {{\textless}p{\textgreater}{\textless}strong{\textgreater}Abstract.{\textless}/strong{\textgreater} Fatigue load assessment of floating offshore wind turbines poses new challenges on the feasibility of numerical procedures. Due to the increased sensitivity of the considered system with respect to the environmental conditions from wind and ocean, the application of common procedures used for fixed-bottom structures results in either inaccurate simulation results or hard-to-quantify conservatism in the system design. Monte Carlo-based sampling procedures provide a more realistic approach to deal with the large variation in the environmental conditions, although basic randomization has shown slow convergence. Specialized sampling methods allow efficient coverage of the complete design space, resulting in faster convergence and hence a reduced number of required simulations. In this study, a quasi-random sampling approach based on Sobol sequences is applied to select representative events for the determination of the lifetime damage. This is calculated applying Monte Carlo integration, using subsets of a resulting total of 16{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}200 coupled time–domain simulations performed with the simulation code FAST. The considered system is the Danmarks Tekniske Universitet (DTU) 10{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}MW reference turbine installed on the LIFES50+ OO-Star Wind Floater Semi 10{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}MW floating platform. Statistical properties of the considered environmental parameters (i.e., wind speed, wave height and wave period) are determined based on the measurement data from the Gulf of Maine, USA. Convergence analyses show that it is sufficient to perform around 200 simulations in order to reach less than 10{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}{\%} uncertainty of lifetime fatigue damage-equivalent loading. Complementary in-depth investigation is performed, focusing on the load sensitivity and the impact of outliers (i.e., values far away from the mean). Recommendations for the implementation of the proposed methodology in the design process are also provided.{\textless}/p{\textgreater}},
author = {M{\"{u}}ller, Kolja and Cheng, Po Wen},
doi = {10.5194/wes-3-149-2018},
file = {:Users/sanderse/Dropbox/work/Mendeley/M{\"{u}}ller, Cheng/Wind Energy Science/M{\"{u}}ller, Cheng - 2018 - Application of a Monte Carlo procedure for probabilistic fatigue design of floating offshore wind turbines.pdf:pdf},
issn = {2366-7451},
journal = {Wind Energy Science},
number = {1},
pages = {149--162},
title = {{Application of a Monte Carlo procedure for probabilistic fatigue design of floating offshore wind turbines}},
volume = {3},
year = {2018}
}
@article{McKay2014,
abstract = {In practical applications of wind energy, ranging from design activities to performance assessment or stability analysis in specific operating conditions and environments, it is critical to have a reliable and efficient modeling methodology. Existing tools allow different insight into the details of the wind turbine and its components, also providing different levels of accuracy and computational effort. From the former work by Voutsinas in 1996 [1] until most recent works [2], panel codes and vortex methods have shown in the last years a good compromise between efficiency and accuracy for aerodynamic modeling, making them very attractive and versatile for practical purposes in combination with Blade Element Momentum (BEM) and Computational Fluid Dynamics (CFD). In this work, some remarks about the development of the new panel code AdaptFoil3D are discussed, together with some details about the implementation. The proposed starting point is based on two reliable tools, OpenFAST for wind turbine modeling and AdaptFoil2D for aerodynamic modeling of 2D airfoils. The idea is to combine efficiency and versatility of engineering codes with improved aerodynamic accuracy of panel codes and vortex methods.},
