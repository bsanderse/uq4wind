journal = {Chemical Engineering Science},
keywords = {Artificial neural network,Flow regimes,Power spectrum density,Pressure fluctuation,Three-phase,Transportability},
number = {11},
pages = {2241--2251},
title = {{Artificial neural network approach for flow regime classification in gas-liquid-fiber flows based on frequency domain analysis of pressure signals}},
volume = {59},
year = {2004}
