@article{Mohebujjaman2019,
abstract = {In our earlier work, we proposed a data-driven filtered reduced order model (DDF-ROM) framework for the numerical simulation of fluid flows, which can be formally written as $\backslash$begin{\{}equation*{\}} $\backslash$boxed{\{} $\backslash$text{\{} DDF-ROM = Galerkin-ROM + Correction {\}} {\}} $\backslash$end{\{}equation*{\}} The new DDF-ROM was constructed by using ROM spatial filtering and data-driven ROM closure modeling (for the Correction term) and was successfully tested in the numerical simulation of a 2D channel flow past a circular cylinder at Reynolds numbers {\$}Re=100, Re=500{\$} and {\$}Re=1000{\$}. In this paper, we propose a {\{}$\backslash$it physically-constrained{\}} DDF-ROM (CDDF-ROM), which aims at improving the physical accuracy of the DDF-ROM. The new physical constraints require that the CDDF-ROM operators satisfy the same type of physical laws (i.e., the nonlinear operator should conserve energy and the ROM closure term should be dissipative) as those satisfied by the fluid flow equations. To implement these physical constraints, in the data-driven modeling step of the DDF-ROM, we replace the unconstrained least squares problem with a constrained least squares problem. We perform a numerical investigation of the new CDDF-ROM and standard DDF-ROM for a 2D channel flow past a circular cylinder at Reynolds numbers {\$}Re=100, Re=500{\$} and {\$}Re=1000{\$}. To this end, we consider a reproductive regime as well as a predictive (i.e., cross-validation) regime in which we use as little as {\$}50\backslash{\%}{\$} of the original training data. The numerical investigation clearly shows that the new CDDF-ROM is significantly more accurate than the DDF-ROM in both regimes.},
author = {Mohebujjaman, M. and Rebholz, L. G. and Iliescu, T.},
doi = {10.1002/fld.4684},
file = {:Users/sanderse/Dropbox/work/Mendeley/Mohebujjaman, Rebholz, Iliescu/International Journal for Numerical Methods in Fluids/Mohebujjaman, Rebholz, Iliescu - 2019 - Physically constrained data-driven correction for reduced-order modeling of fluid flows.pdf:pdf},
issn = {10970363},
journal = {International Journal for Numerical Methods in Fluids},
keywords = {correction,data-driven modeling,physical constraints,reduced-order model,spatial filter},
number = {3},
pages = {103--122},
title = {{Physically constrained data-driven correction for reduced-order modeling of fluid flows}},
volume = {89},
year = {2019}
}
@article{Valle2019a,
abstract = {The computation of multiphase flows presents a subtle energetic equilibrium between potential (i.e., surface) and kinetic energies. The use of traditional interface-capturing schemes provides no control over such a dynamic balance. In the spirit of the wellknown symmetry-preserving and mimetic schemes, whose physics-compatible discretizations rely upon preserving the underlying mathematical structures of the space, we identify the corresponding structure and propose a new discretization strategy for curvature. The new scheme ensures conservation of mechanical energy (i.e., surface plus kinetic) up to temporal integration. Inviscid numerical simulations are performed to show the robustness of such a method.},
archivePrefix = {arXiv},
arxivId = {1909.01114},
author = {Valle, N. and Trias, F. X. and Castro, J.},
