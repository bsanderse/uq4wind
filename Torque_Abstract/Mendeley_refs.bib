doi = {10.1111/1467-9469.00064},
file = {:Users/sanderse/Dropbox/work/Mendeley/Chauveau, Vandekerkhove/Scandinavian Journal of Statistics/Chauveau, Vandekerkhove - 2002 - Improving convergence of the Hastings-Metropolis algorithm with an adaptive proposal.pdf:pdf},
issn = {03036898},
journal = {Scandinavian Journal of Statistics},
keywords = {Adaptive algorithm,Geometric ergodicity,Hastings-Metropolis algorithm,Markov chain Monte Carlo},
number = {1},
pages = {13--29},
title = {{Improving convergence of the Hastings-Metropolis algorithm with an adaptive proposal}},
volume = {29},
