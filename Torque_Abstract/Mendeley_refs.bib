journal = {2010 IEEE 11th International Conference on Probabilistic Methods Applied to Power Systems},
keywords = {- wind turbine generator,curve,power,renewable energy,uncertainty estimation},
pages = {745--750},
title = {{Uncertainty analysis for wind energy production with dynamic power curves}},
