@article{Fluck2018,
abstract = {Unsteady power output and long-term loads (extreme and fatigue) drive wind turbine design. However, these loads are difficult to include in optimization loops and are typically only assessed in a post-optimization load analysis or via reduced-order methods. Both alternatives yield suboptimal results. The reason for this difficulty lays in the deterministic approaches to long-term loads assessment. To model the statistics of lifetime loads they require the analysis of many unsteady load cases, generated from many different random seeds—a computationally expensive procedure. In this paper, we present an alternative: a stochastic solution for the unsteady aerodynamic loads based on a projection of the unsteady Blade Element Momentum (BEM) equations onto a stochastic space spanned by chaos exponentials. This approach is similar to the increasingly popular polynomial chaos expansion, but with 2 major differences. First, the BEM equations constitute a random process, varying in time, while previous polynomial chaos expansion methods were concerned with random parameters (ie, random but constant in time or initial values). Second, a new, more efficient basis (the exponential chaos) is used. This new stochastic method enables us to obtain unsteady long-term loads much faster, enabling unsteady loads to become accessible inside wind turbine optimization loops. In this paper we derive the stochastic BEM solution and present the most relevant results showing the accuracy of the new method.},
author = {Fluck, Manuel and Crawford, Curran},
doi = {10.1002/we.2148},
file = {:Users/sanderse/Dropbox/work/Mendeley/Fluck, Crawford/Wind Energy/Fluck, Crawford - 2018 - A fast stochastic solution method for the Blade Element Momentum equations for long-term load assessment.pdf:pdf},
issn = {10991824},
journal = {Wind Energy},
keywords = {Blade Element Momentum,aerodynamic,chaos expansion,extreme loads,fatigue loads,stochastic,wind turbine},
number = {2},
pages = {115--128},
title = {{A fast stochastic solution method for the Blade Element Momentum equations for long-term load assessment}},
volume = {21},
year = {2018}
}
@article{Bortolotti2019,
abstract = {The present paper characterizes the performance of non-intrusive uncertainty quantification methods for aeroservoelastic wind turbine analysis. Two different methods are considered, namely non-intrusive polynomial chaos expansion and Kriging. Aleatory uncertainties are associated with the wind inflow characteristics and the blade surface state, on account of soiling and/or erosion, and propagated throughout the aeroservoelastic model of a large conceptual offshore wind turbine. Results are compared with a brute-force extensive Monte Carlo sampling, which is used as benchmark. Both methods require at least 1 order of magnitude less simulations than Monte Carlo, with a slight advantage of Kriging over polynomial chaos expansion. The analysis of the solution space clearly indicates the effects of uncertainties and their couplings, and highlights some possible shortcomings of current mostly deterministic approaches based on safety factors.},
author = {Bortolotti, Pietro and Canet, Helena and Bottasso, Carlo L. and Loganathan, Jaikumar},
doi = {10.5194/wes-4-397-2019},
file = {:Users/sanderse/Dropbox/work/Mendeley/Bortolotti et al/Wind Energy Science/Bortolotti et al. - 2019 - Performance of non-intrusive uncertainty quantification in the aeroservoelastic simulation of wind turbines.pdf:pdf},
issn = {23667451},
journal = {Wind Energy Science},
number = {3},
pages = {397--406},
title = {{Performance of non-intrusive uncertainty quantification in the aeroservoelastic simulation of wind turbines}},
volume = {4},
year = {2019}
}
@article{Echeverria2017,
abstract = {In practical applications of wind energy, ranging from design activities to performance assessment or stability analysis in specific operating conditions and environments, it is critical to have a reliable and efficient modeling methodology. Existing tools allow different insight into the details of the wind turbine and its components, also providing different levels of accuracy and computational effort. From the former work by Voutsinas in 1996 [1] until most recent works [2], panel codes and vortex methods have shown in the last years a good compromise between efficiency and accuracy for aerodynamic modeling, making them very attractive and versatile for practical purposes in combination with Blade Element Momentum (BEM) and Computational Fluid Dynamics (CFD). In this work, some remarks about the development of the new panel code AdaptFoil3D are discussed, together with some details about the implementation. The proposed starting point is based on two reliable tools, OpenFAST for wind turbine modeling and AdaptFoil2D for aerodynamic modeling of 2D airfoils. The idea is to combine efficiency and versatility of engineering codes with improved aerodynamic accuracy of panel codes and vortex methods.},
author = {Echeverr{\'{i}}a, Fernando and Mallor, Ferm{\'{i}}n and {San Miguel}, Unai},
doi = {10.1002/we.2111},
file = {:Users/sanderse/Dropbox/work/Mendeley/Echeverr{\'{i}}a, Mallor, San Miguel/Wind Energy/Echeverr{\'{i}}a, Mallor, San Miguel - 2017 - Global sensitivity analysis of the blade geometry variables on the wind turbine performance.pdf:pdf},
issn = {10954244},
journal = {Wind Energy},
keywords = {aerodynamics,hawt,load estimation,panel code},
month = {sep},
number = {9},
pages = {1601--1616},
title = {{Global sensitivity analysis of the blade geometry variables on the wind turbine performance}},
url = {http://doi.wiley.com/10.1002/we.2111},
volume = {20},
year = {2017}
}
@article{Ribeiro2012,
abstract = {Optimization algorithms coupled with computational fluid dynamics are used for wind turbines airfoils design. This differs from the traditional aerospace design process since the lift-to-drag ratio is the most important parameter and the angle of attack is large. Computational fluid dynamics simulations are performed with the incompressible Reynolds-averaged Navier-Stokes equations in steady state using a one equation turbulence model. A detailed validation of the simulations is presented and a computational domain larger than suggested in literature is shown to be necessary. Different approaches to parallelization of the computational code are addressed. Single and multiobjective genetic algorithms are employed and artificial neural networks are used as a surrogate model. The use of artificial neural networks is shown to reduce computational time by almost 50{\%}. {\textcopyright} 2011 Elsevier Inc..},
author = {Ribeiro, A. F.P. and Awruch, A. M. and Gomes, H. M.},
doi = {10.1016/j.apm.2011.12.026},
file = {:Users/sanderse/Dropbox/work/Mendeley/Ribeiro, Awruch, Gomes/Applied Mathematical Modelling/Ribeiro, Awruch, Gomes - 2012 - An airfoil optimization technique for wind turbines.pdf:pdf},
issn = {0307904X},
journal = {Applied Mathematical Modelling},
keywords = {Artificial neural networks,Computational fluid dynamics,Genetic algorithms},
number = {10},
pages = {4898--4907},
publisher = {Elsevier Inc.},
title = {{An airfoil optimization technique for wind turbines}},
volume = {36},
year = {2012}
}
