volume = {37},
year = {2011}
}
@article{Benjamin1968a,
abstract = {Broadly investigates the properties of steady gravity currents using a prototype model which neglects the effects of viscosity and mixing of the fluids at the interface. Energy conserving flow and flow with energy loss are considered together with the problem of liquid emptying from a horizontal tube. Further properties of 2D energy conserving flow are dealt with, and it is shown that a head wave is impossible. Next, there is an investigation of non-uniform asymptotic currents followed by a treatment of the case of great depth in which wave breaking is discussed, propagation velocity is derived and comparison with experiment is made. Finally, the properties of the wake are numerically estimated.},
