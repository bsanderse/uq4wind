issn = {13616420},
journal = {Inverse Problems},
keywords = {PDE-constrained optimization,deep neural networks,dynamic inverse problems,image classification,machine learning,parameter estimation},
number = {1},
title = {{Stable architectures for deep neural networks}},
volume = {34},
year = {2018}
}
@article{Jay2007,
