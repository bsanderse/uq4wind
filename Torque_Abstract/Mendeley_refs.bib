isbn = {9780791848234},
issn = {08927219},
journal = {Journal of Offshore Mechanics and Arctic Engineering},
title = {{On the modeling of nonlinear waves for prediction of long-term offshore wind turbine loads}},
volume = {131},
year = {2009}
}
