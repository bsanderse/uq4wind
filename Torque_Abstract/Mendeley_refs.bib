file = {:Users/sanderse/Dropbox/work/Mendeley/Geneva, Zabaras/Unknown/Geneva, Zabaras - 2019 - Modeling the Dynamics of PDE Systems with Physics-Constrained Deep Auto-Regressive Networks.pdf:pdf},
