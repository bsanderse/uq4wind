pages = {294--303},
publisher = {Elsevier B.V},
title = {{Dynamic analysis and simulation of long pig in gas pipeline}},
url = {http://dx.doi.org/10.1016/j.jngse.2015.02.004},
volume = {23},
year = {2015}
