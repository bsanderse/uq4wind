journal = {50th AIAA Aerospace Sciences Meeting},
keywords = {January 2012,NREL/CP-5000-53554,atmospheric turbulence,large-eddy simulation,wind farm aerodynamics,wind plant aerodynamics,wind turbine},
number = {January},
pages = {AIAA 2012--0537},
title = {{A Large-Eddy Simulation of Wind-Plant Aerodynamics Preprint}},
year = {2012}
