keywords = {62f10,65l05,ams subject classifications,discretization error,isotonic regression,parameter estimation,probabilistic numerics},
title = {{Estimation of ordinary differential equation models with discretization error quantification}},
url = {http://arxiv.org/abs/1907.10565},
year = {2019}
}
