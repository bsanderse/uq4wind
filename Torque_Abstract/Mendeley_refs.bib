issn = {10902716},
journal = {Journal of Computational Physics},
keywords = {Difference equations,Immersed boundary method,Incompressible viscous flow,Lattice Green's function,Projection method,Unbounded domain},
pages = {257--279},
title = {{A fast immersed boundary method for external incompressible viscous flows using lattice Green's functions}},
