file = {:Users/sanderse/Dropbox/work/Mendeley/Nordstr{\"{o}}m/Journal of Scientific Computing/Nordstr{\"{o}}m - 2017 - A Roadmap to Well Posed and Stable Problems in Computational Physics.pdf:pdf},
