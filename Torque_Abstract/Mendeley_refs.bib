keywords = {Bubbles,Drops,Incompressible two phase flows,Jets,Level set method,Volume constraint},
number = {19},
pages = {6438--6471},
publisher = {Elsevier Inc.},
title = {{A hybrid level set-volume constraint method for incompressible two-phase flow}},
url = {http://dx.doi.org/10.1016/j.jcp.2012.06.014},
volume = {231},
