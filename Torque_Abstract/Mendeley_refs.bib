doi = {10.1016/j.jcp.2011.08.004},
file = {:Users/sanderse/Dropbox/work/Mendeley/Tartakovsky, Panchenko, Ferris/Journal of Computational Physics/Tartakovsky, Panchenko, Ferris - 2011 - Dimension reduction method for ODE fluid models.pdf:pdf},
